{
  "name": "orw",
  "module": "orw.ts",
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "start:clean": "bun run dist-clean && bun run start",
    "start": "bun install && bun run build && bun run orw.ts",
    "build": "vite build --mode production",
    "watch:vite-standalone": "vite build --watch --mode development",
    "watch:vite": "vite --mode development --open",
    "watch": "bun run --hot orw.ts & bun run watch:vite",
    "query": "bun run orw.ts --query",
    "once": "bun run orw.ts --once",
    "dist-clean": "rm -fR dist cache",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "screenshots": "cd screenshots && for i in *.png; do zopflipng -y $i $i; done && cd .."
  },
  "devDependencies": {
    "@kitsuyui/luxon-ext": "^0.1.5",
    "@types/bun": "^1.1.3",
    "@types/deep-diff": "^1.0.5",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/rss": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "babel-plugin-react-compiler": "^0.0.0-experimental-592953e-20240517",
    "eslint": "^9.0.0",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-c8b3f72-20240517",
    "eslint-plugin-react-hooks": "beta",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.0.0",
    "luxon": "^3.4.4",
    "react": "next",
    "react-data-table-component": "^7.6.2",
    "react-dom": "next",
    "react-router-dom": "^6.23.1",
    "styled-components": "^6.1.11"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "deep-diff": "^1.0.2",
    "rss": "^1.2.2",
    "vite": "^5.2.11",
    "vite-plugin-compression2": "^1.1.1"
  }
}