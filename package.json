{
  "name": "orw",
  "module": "orw.ts",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build --mode production",
    "watch:vite-standalone": "vite build --watch --mode development",
    "watch:vite": "vite --mode development --open",
    "watch": "bun run --hot orw.ts & bun run watch:vite",
    "start": "bun run orw.ts",
    "query": "bun run orw.ts --query",
    "once": "bun run orw.ts --once",
    "gzip": "rm -f ./static/*.gz && gzip --best -k ./static/*",
    "dist-clean": "rm -fR dist cache",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "screenshots": "cd screenshots && for i in *.png; do zopflipng -y $i $i; done && cd .."
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/deep-diff": "^1.0.5",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/rss": "^0.0.32",
    "@kitsuyui/luxon-ext": "^0.1.5",
    "luxon": "^3.4.4",
    "react": "^18.3.1",
    "react-data-table-component": "^7.6.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "styled-components": "^6.1.11"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "deep-diff": "^1.0.2",
    "rss": "^1.2.2",
    "vite": "^5.2.11",
    "vite-plugin-compression2": "^1.1.0"
  }
}